<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.ly.testwebview.WebviewCacheActivity">

    <Button
        android:id="@+id/btn_load_android_to_js"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="load"/>


    <Button
        android:id="@+id/btn_webview_cache"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="webviewCache(自行实现,无需处理)"/>

    <Button
        android:id="@+id/btn_application_cache"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Application Cache"/>

    <Button
        android:id="@+id/btn_dom_storage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Dom Storage"/>

    <Button
        android:id="@+id/btn_indexedDB"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="IndexedDB"/>

    <Button
        android:id="@+id/btn_setCachePattern"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="setCachepattern"/>

    <Button
        android:id="@+id/btn_customCache"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="customCache"/>

    <FrameLayout
        android:id="@+id/layout_webview_cache"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>
